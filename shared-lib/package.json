{
    "name": "shared-lib",
    "version": "1.0.0",
    "description": "",
    "types": "esm/index.d.ts",
    "type": "module",
    "exports": {
        "./util": {
            "require": "./cjs/util.js",
            "import": "./esm/util.js",
            "types": "./esm/util.d.ts"
        },
        "./base_util": {
            "require": "./cjs/base_util.js",
            "import": "./esm/base_util.js",
            "types": "./esm/base_util.d.ts"
        },
        "./database": {
            "require": "./cjs/database.js",
            "import": "./esm/database.js",
            "types": "./esm/database.d.ts"
        },
        "./user": {
            "require": "./cjs/user.js",
            "import": "./esm/user.js",
            "types": "./esm/user.d.ts"
        },
        "./gd": {
            "require": "./cjs/gd.js",
            "import": "./esm/gd.js",
            "types": "./esm/gd.d.ts"
        },
        "./cloud_functions": {
            "require": "./cjs/cloud_functions.js",
            "import": "./esm/cloud_functions.js",
            "types": "./esm/cloud_functions.d.ts"
        },
        "./kofi": {
            "require": "./cjs/kofi.js",
            "import": "./esm/kofi.js",
            "types": "./esm/kofi.d.ts"
        },
        ".": {
            "require": "./cjs/index.js",
            "import": "./esm/index.js",
            "types": "./esm/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc --module commonjs --outDir cjs/ && tsc --module esnext --outDir esm/"
