{
    "compilerOptions": {
        "module": "nodenext",
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "outDir": "lib",
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
    },
    "compileOnSave": true,
    "include": ["src"],
}
